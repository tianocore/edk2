[Defines]
  INF_VERSION                    = 0x00010005
  BASE_NAME                      = PrintDxe
  MODULE_UNI_FILE                = PrintDxe.uni
  FILE_GUID                      = 79E4A61C-ED73-4312-94FE-E3E7563362A9
