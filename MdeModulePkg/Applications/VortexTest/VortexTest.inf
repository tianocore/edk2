[Defines]
  INF_VERSION                    = 0x00010005
  BASE_NAME                      = VortexTest
  FILE_GUID                      = 87654321-4321-8765-4321-876543218765
  MODULE_TYPE                    = UEFI_APPLICATION
  VERSION_STRING                 = 1.0
  ENTRY_POINT                    = ShellAppMain

[Sources]
  VortexTest.c

[Packages]
  MdePkg/MdePkg.dec
  MdeModulePkg/MdeModulePkg.dec

[LibraryClasses]
  UefiApplicationEntryPoint
  UefiLib
  UefiBootServicesTableLib
  BaseLib
  DebugLib
  PrintLib

[BuildOptions]
  MSFT:*_*_*_CC_FLAGS = /fp:strict /GS-
  MSFT:*_*_*_DLINK_FLAGS = /NODEFAULTLIB /SAFESEH:NO
  GCC:*_*_*_CC_FLAGS = -std=c99 -ffreestanding
