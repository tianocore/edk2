                  <xs:documentation xml:lang="en-us">
                    Only required if different from the Package
                    License.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="Abstract" type="xs:string">
